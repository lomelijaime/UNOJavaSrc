            <Property name="text" type="java.lang.String" editor="org.netbeans.modules.form.RADConnectionPropertyEditor">
              <Connection code="&quot;&lt;html&gt;&lt;b&gt;Carta Roba 2:&lt;/b&gt; cuando se tira esta carta, &lt;br&gt;el siguiente jugador roba dos\n&quot; +&#xa;    &quot;cartas y pierde su turno.&lt;br&gt; Esta carta s&#xf3;lo se puede descartar sobre una&lt;br&gt;&quot; +&#xa;    &quot;carta del mismo color o sobre otra carta Roba 2.&lt;br&gt; Si &#xe9;sta es la primera\n&quot; +&#xa;    &quot;carta del juego que se voltea,&lt;br&gt; se sigue la misma regla de juego.&lt;html&gt;&quot;" type="code"/>
            </Property>
          </Properties>
        </Component>
